{
  "manifest_version": 2,

  "name": "PINTAB",
  "description": "Saves Tabs and opens up it pinned.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup":"app/index.html"
  },
  "background": {
    "page": "background.html",
    "persistent": true

  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "tabs",
    "storage"
  ]
}
